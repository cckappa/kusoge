config_version=5

[application]

config/name="kusoge"
config/tags=PackedStringArray("current")
run/main_scene="res://scenes/inicio_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://kusoge_icon.png"

[autoload]

DevTools="*res://scripts/dev_tools.gd"
ConfigFileHandler="*res://scripts/config_file_handler.gd"
SignalBus="*res://scripts/signal_bus.gd"
Functions="*res://scripts/functions.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[debug]

gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0
gdscript/warnings/untyped_declaration=2

[dialogic]

directories/dch_directory={}
directories/dtl_directory={
"obj_mecha": "res://dialogic/timelines/obj_mecha.dtl",
"obj_table_hospital": "res://dialogic/timelines/obj_table_hospital.dtl"
}

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[global_group]

main_character="The main character"
objects="Object Group"

[gui]

theme/default_font_multichannel_signed_distance_field=true

